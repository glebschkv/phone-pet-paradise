# ===========================================
# NoMo Phone - Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# -------------------------------------------
# Supabase Configuration (Required)
# -------------------------------------------
# Get these values from: https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_PROJECT_ID="your-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-anon-key"
VITE_SUPABASE_URL="https://your-project-id.supabase.co"

# -------------------------------------------
# App Configuration (Required for Production)
# -------------------------------------------
# Your production app URL (used for OAuth redirects)
VITE_APP_URL="https://your-production-domain.com"

# -------------------------------------------
# Error Tracking - Sentry (Required for Production)
# -------------------------------------------
# Get your DSN from: https://sentry.io/settings/projects/YOUR_PROJECT/keys/
VITE_SENTRY_DSN=""
# Optional: Set to your app version for release tracking
VITE_APP_VERSION="1.0.0"

# -------------------------------------------
# Environment (auto-detected, but can override)
# -------------------------------------------
# Set to "production" for production builds
# VITE_ENVIRONMENT="production"
