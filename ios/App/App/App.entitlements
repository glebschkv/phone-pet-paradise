<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Required for Screen Time API -->
	<key>com.apple.developer.family-controls</key>
	<true/>
	<!-- Required for DeviceActivity monitoring -->
	<key>com.apple.developer.device-activity</key>
	<true/>
	<!-- Required for App Groups if using shared data -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.co.nomoinc.nomo</string>
	</array>
	<!-- Required for In-App Purchases -->
	<key>com.apple.developer.in-app-payments</key>
	<array>
		<string>merchant.co.nomoinc.nomo</string>
	</array>
</dict>
</plist>